                                                                                                   <div class='menu-container'>
  <div id="travelContainer"class='card-header col-md-10'>
    <h2>
      Travelogues
      <br />
       <div class='add-travel-note'></div>
        </h2>
    <h2>
      Enter a trip to remember
    </h2>
  </div>
  <div>
 {{#each travelDatas as |travelData|}}
    <div class='travel col-md-5'>
      <p>
        {{travelData.destination}}.
        {{travelData.note}}
      </p>
    </div>
    {{!-- <div class='col-md-7'>
      <p>
    <a href="travelogue/1">{{travelData.destination}}</a>

        {{travelData.date}} 
      </p>
    </div> --}}
  {{/each}}
  </div>
  <br />
   <div>
    <form class='new-post-form'>
      <label for='destination_name'>
        Destination Name:
      </label>
      <br />
      <input type = 'text' id="destination" name="destination"/>
      <label for='note'>
        Travel Notes:
      </label>
      <br />
      <textarea id='note' name='note' rows='5' cols='30'>
        {{!-- {{note}} --}}
      </textarea>
      <br />
      <label for="date">
        Date:
      </label>
      <br />
      <input type="date" id="date" name="date" rows='2' cols='30'>
        {{!-- {{note}} --}}
      <br />
      <button type='submit' value='Submit'>
        Submit
      </button>
    </form>
  </div>
<!-- This is the form for adding a new log-->
<script src='/js/addTravel.js'></script>
